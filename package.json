{
  "name": "django-react-redux-hmre-base",
  "version": "1.0.0",
  "description": "Django, React, Redux with React Hot Loader",
  "author": "Anthony Tran <atran.dummy@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atran/django-react-redux-hmre-base.git"
  },
  "scripts": {
    "dev": "node devServer.js",
    "build": "cross-env NODE_ENV=production webpack --progress -p --config webpack/prod.config.js",
    "lint": "npm run lint:js && npm run lint:scss && prospector --messages-only",
    "lint:js": "npm run lint:eslint",
    "lint:eslint": "eslint --ext js,jsx static/src/.",
    "lint:scss": "scss-lint ./static/src/**/**/*.scss"
  },
  "pre-push": [
    "lint"
  ],
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-custom-media": {},
      "postcss-cssnext": {
        "browsers": [
          "last 2 versions",
          "> 5%"
        ]
      },
      "lost": {}
    }
  },
  "dependencies": {
    "autoprefixer": "6.5.4",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-undeclared-variables-check": "6.8.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-runtime": "6.20.0",
    "body-parser": "1.15.2",
    "classnames": "2.2.5",
    "css-loader": "^0.26.1",
    "dotenv": "2.0.0",
    "errorhandler": "1.5.0",
    "es6-promise": "4.0.5",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "history": "4.5.0",
    "html-webpack-plugin": "2.24.1",
    "imports-loader": "0.7.0",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.1.1",
    "method-override": "2.3.7",
    "node-sass": "3.13.1",
    "postcss-import": "^9.1.0",
    "postcss-loader": "1.2.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-mixin": "3.0.5",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "resolve-url-loader": "1.6.1",
    "sass-loader": "4.1.0",
    "style-loader": "^0.13.1",
    "tcomb-form": "0.9.10",
    "url-loader": "0.5.7",
    "webpack": "2.2.0-rc.3",
    "webpack-merge": "1.1.1",
    "whatwg-fetch": "2.0.1",
    "yargs": "6.5.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "clean-webpack-plugin": "0.1.14",
    "cross-env": "^3.1.4",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "expect": "1.20.2",
    "ignore-styles": "5.0.01",
    "ip": "^1.1.4",
    "jsdom": "9.8.3",
    "lost": "^8.0.0",
    "nock": "9.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-load-config": "^1.1.0",
    "pre-push": "0.1.1",
    "react-addons-test-utils": "15.4.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.3.3",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.1.1",
    "redux-logger": "2.7.4",
    "redux-mock-store": "1.2.1",
    "sass-lint": "1.10.2",
    "webpack-bundle-tracker": "^0.2.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-dev-server": "^2.3.0",
